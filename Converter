SetWorkingDir  A_ScriptDir
try  FileDelete  "Text.txt"
catch as e 
{
x := 1
}

IB := InputBox("Please input an RGB color in Hex with no #. `n`n ie 3f9de2")
color1 := IB.value

IB := InputBox("Color 2")
color2 := IB.Value


IB := InputBox("Color 3")
