 SelectedFile := FileSelect(3, , "Open a file", "Text Documents (*.txt; *.doc)") 
  Loop read, SelectedFile
 {
 If A_LoopReadLine = "e"
{
 FileAppend "`n" , "Text.txt", "UTF-8"

 try  TextTemp := FileRead("Temp.txt")
catch as e 
{
x := 1
